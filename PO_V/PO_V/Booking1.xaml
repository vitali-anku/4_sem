        <ComboBox x:Name="comboBox" HorizontalAlignment="Left" Margin="320,290,0,0" VerticalAlignment="Top" Width="120" >
            <TextBlock>Витебск</TextBlock>
            <TextBlock>Гродно</TextBlock>
            <TextBlock>Брест</TextBlock>
            <TextBlock>Гомель</TextBlock>
            <TextBlock>Могилёв</TextBlock>
        </ComboBox>

    </Grid>
</Page>
