<Page x:Class="PO_V.Sign_up"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:PO_V"
      mc:Ignorable="d" 
      d:DesignHeight="600" d:DesignWidth="800"
      Title="Sign_up">

    <Grid>
        <Button x:Name="button" Content="Зарегистрироваться" HorizontalAlignment="Left" Margin="370,455,0,0" VerticalAlignment="Top" Width="140" Height="35" RenderTransformOrigin="0.222,0" Click="button_Click"/>
        <Label x:Name="label5" Content="password:" HorizontalAlignment="Left" Margin="375,216,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="23" Margin="435,165,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="textBox1" HorizontalAlignment="Left" Height="23" Margin="435,193,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <PasswordBox Name="passwordBox" PasswordChar="*" HorizontalAlignment="Left" Margin="435,222,0,0" Height="20" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="label" Content="email:" HorizontalAlignment="Left" Margin="397,163,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label1" Content="name:" HorizontalAlignment="Left" Margin="396,190,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label2" Content="login:" HorizontalAlignment="Left" Margin="399,135,0,0" VerticalAlignment="Top" Height="28"/>
        <TextBox x:Name="textBox3"   HorizontalAlignment="Left" Height="23" Margin="435,137,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="120"/>
    </Grid>
</Page>
